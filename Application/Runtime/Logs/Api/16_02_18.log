SQL: SELECT * FROM `zuke_case` WHERE ( topic_id=45 AND content='气体实验定律与热力学定律的综合问题的处理方法&lt;br /&gt;(1)气体实验定律研究对象是一定质量的理想气体．&lt;br /&gt;(2)解决具体问题时，分清气体的变化过程是求解问题的关键，根据不同的变化，找出与之相关的气体状态参量，利用相关规律解决．&lt;br /&gt;(3)对理想气体，只要体积变化，外界对气体(或气体对外界)要做功，如果是等压变化，W＝pΔV；只要温度发生变化，其内能就发生变化．&lt;br /&gt;(4)结合热力学第一定律ΔU＝W＋Q求解问题．' ) LIMIT 1   [ RunTime:0.0000s ]
SQL: INSERT INTO `zuke_case` (`topic_id`,`content`,`update_time`,`resource_id`,`round_id`) VALUES ('45','气体实验定律与热力学定律的综合问题的处理方法&lt;br /&gt;(1)气体实验定律研究对象是一定质量的理想气体．&lt;br /&gt;(2)解决具体问题时，分清气体的变化过程是求解问题的关键，根据不同的变化，找出与之相关的气体状态参量，利用相关规律解决．&lt;br /&gt;(3)对理想气体，只要体积变化，外界对气体(或气体对外界)要做功，如果是等压变化，W＝pΔV；只要温度发生变化，其内能就发生变化．&lt;br /&gt;(4)结合热力学第一定律ΔU＝W＋Q求解问题．','1455786820','3','1') [ RunTime:0.0000s ]

SQL: INSERT INTO `zuke_case` (`topic_id`,`content`,`update_time`,`resource_id`,`round_id`) VALUES ('53','对光电效应的“四点提醒”：&lt;br /&gt;(1)能否发生光电效应，不取决于光的强度而取决于光的频率．&lt;br /&gt;(2)光电效应中的“光”不是特指可见光，也包括不可见光．&lt;br /&gt;(3)逸出功的大小由金属本身决定，与入射光无关．&lt;br /&gt;(4)光电子不是光子，而是电子．','1455845182','3','1') [ RunTime:0.0000s ]

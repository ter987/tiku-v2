SQL: INSERT INTO `zuke_case` (`topic_id`,`content`,`update_time`,`resource_id`,`round_id`) VALUES ('37','导体棒切割磁感线类问题要注意以下几点&lt;br /&gt;（1）确定切割磁感线的导体棒是平动切割还是转动切割．若是平动切割，注意切割的有效长度；若是转动切割，导体各部分切割速度不同，应用E＝BLvsinθ计算电动势时，v应是导体棒切割的平均速度．&lt;br /&gt;（2）导体棒切割磁感线相当于电源，要注意感应电动势的方向及电路的结构．','1455788857','3','1') [ RunTime:0.0000s ]
